<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.phn.bean.Grades" table="t_grade" batch-size="5">
		<id name="id" type="int">
			<column name="id" />
			<generator class="increment"></generator>
		</id>
		<property name="honor" type="string" column="honor" length="30">
		</property>
	</class>
</hibernate-mapping>
